 {\r\n        return HTTP.put(BASE_URL + `/users/${user.id}`, user, { headers: authHeader() }).then(\r\n            (response) => {\r\n                return response.data;\r\n            }\r\n        );\r\n    },\r\n\r\n    delete(user){\r\n        return HTTP.delete(BASE_URL + `/users/${user.id}`, { headers: authHeader() }).then(\r\n            (response) => {\r\n                return response.data;\r\n            }\r\n        );\r\n    },\r\n};"]}]}