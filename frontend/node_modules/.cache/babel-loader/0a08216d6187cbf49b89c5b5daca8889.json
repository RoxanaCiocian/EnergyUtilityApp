ensorDialog },\r\n  data() {\r\n    return {\r\n      users: [],\r\n      search: \"\",\r\n      headers: [\r\n        {\r\n          text: \"ID\",\r\n          align: \"start\",\r\n          sortable: false,\r\n          value: \"id\",\r\n        },\r\n        { text: \"Description\", value: \"description\" },\r\n        { text: \"Maximum value\", value: \"maxValue\" },\r\n        { text: \"Device ID\", value: \"deviceId\" },\r\n        // { text: \"Roles\", value: \"roles\" },\r\n      ],\r\n      dialogVisible: false,\r\n      selectedUser: {},\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    editUser(user) {\r\n      this.selectedUser = user;\r\n      this.dialogVisible = true;\r\n    },\r\n    addUser() {\r\n      this.dialogVisible = true;\r\n    },\r\n    async refreshList() {\r\n      this.dialogVisible = false;\r\n      this.selectedUser = {};\r\n      this.users = await api.sensors.allSensors();\r\n    },\r\n    goToUsers() {\r\n      router.push(\"/users\");\r\n    },\r\n    goToDevices() {\r\n        router.push(\"/devices\");\r\n    },\r\n  },\r\n\r\n  async created() {\r\n    this.users = await api.sensors.allSensors();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped></style>\r\n"],"sourceRoot":"src/views"}]}