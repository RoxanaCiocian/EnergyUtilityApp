mport api from \"../api\";\nexport default {\n  name: \"SensorDialog\",\n  props: {\n    sensor: Object,\n    opened: Boolean,\n  },\n  methods: {\n    persist() {\n      if (this.isNew) {\n        api.sensors\n            .create({\n              description: this.sensor.description,\n              maxValue: this.sensor.maxValue,\n              deviceId: this.sensor.deviceId,\n            })\n            .then(() => this.$emit(\"refresh\"));\n      } else {\n        api.sensors\n            .update({\n              id: this.sensor.id,\n              description: this.sensor.description,\n              maxValue: this.sensor.maxValue,\n              deviceId: this.sensor.deviceId,\n            })\n            .then(() => this.$emit(\"refresh\"));\n      }\n    },\n\n    deleteSensor(){\n      api.sensors\n          .delete({\n            id: this.sensor.id,\n          })\n          .then(() => this.$emit(\"refresh\"));\n    },\n  },\n  computed: {\n    isNew: function () {\n      return !this.sensor.id;\n    },\n  },\n};\n</script>\n\n<style scoped></style>"],"sourceRoot":"src/components"}]}