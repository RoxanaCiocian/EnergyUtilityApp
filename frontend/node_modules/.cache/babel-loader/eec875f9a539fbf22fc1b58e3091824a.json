vice){\n        return HTTP.delete(BASE_URL + `/devices/${device.id}`, { headers: authHeader() }).then(\n            (response) => {\n                return response.data;\n            }\n        );\n    },\n    addUserToDevice(device, userId) {\n        return HTTP.put(BASE_URL + `/devices/${device.id}`, userId, {headers: authHeader()}).then(\n            (response) => {\n                return response.data;\n            }\n        );\n    },\n\n    addSensorToDevice(device, sensorId) {\n        return HTTP.put(BASE_URL + `/devices/${device.id}`, sensorId, {headers: authHeader()}).then(\n            (response) => {\n                return response.data;\n            }\n        );\n    }\n};"]}]}