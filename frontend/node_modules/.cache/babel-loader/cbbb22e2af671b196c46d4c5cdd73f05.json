ess\" label= \"Address\" />\r\n          <v-text-field v-model=\"user.age\" label= \"Age\" />\r\n          <v-text-field v-model=\"user.role\" label= \"Role\" />\r\n        </v-form>\r\n        <v-card-actions>\r\n          <v-btn @click=\"persist\">\r\n            {{ isNew ? \"Create\" : \"Save\" }}\r\n          </v-btn>\r\n          <v-btn @click=\"deleteUser\">Delete User</v-btn>\r\n        </v-card-actions>\r\n      </v-card>\r\n    </template>\r\n  </v-dialog>\r\n</template>\r\n\r\n<script>\r\nimport api from \"../api\";\r\nexport default {\r\n  name: \"UserDialog\",\r\n  props: {\r\n    user: Object,\r\n    opened: Boolean,\r\n  },\r\n  methods: {\r\n    persist() {\r\n      if (this.isNew) {\r\n        api.users\r\n            .create({\r\n              name: this.user.username,\r\n              email: this.user.email,\r\n              password: this.user.password,\r\n              role: this.user.role,\r\n              address: this.user.address,\r\n              age:this.user.age,\r\n            })\r\n            .then(() => this.$emit(\"refresh\"));\r\n      } else {\r\n        api.users\r\n            .update({\r\n              id: this.user.id,\r\n              name: this.user.username,\r\n              email: this.user.email,\r\n              role: this.user.role,\r\n              address: this.user.address,\r\n              age:this.user.age,\r\n            })\r\n            .then(() => this.$emit(\"refresh\"));\r\n      }\r\n    },\r\n\r\n    deleteUser(){\r\n      api.users\r\n          .delete({\r\n            id: this.user.id,\r\n          })\r\n          .then(() => this.$emit(\"refresh\"));\r\n    },\r\n  },\r\n  computed: {\r\n    isNew: function () {\r\n      return !this.user.id;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped></style>"],"sourceRoot":"src/components"}]}